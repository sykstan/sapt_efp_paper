@Article{Milet1999a,
  author =     {Milet, Anne and Moszynski, Robert and Wormer, Paul E. S. and van der Avoird, Ad},
  title =      {Hydrogen {Bonding} in {Water} {Clusters}: {Pair} and {Many}-{Body} {Interactions} from {Symmetry}-{Adapted} {Perturbation} {Theory}},
  journal =    {The Journal of Physical Chemistry A},
  year =       {1999},
  volume =     {103},
  number =     {34},
  pages =      {6811--6819},
  month =      aug,
  abstract =   {This paper contains a study of the pair and many-body interactions in cyclic water clusters:? trimer, tetramer, and pentamer. Symmetry-adapted perturbation theory (SAPT) is applied to compute the pair- and three-body interactions directly and to analyze the individual electrostatic, induction, dispersion, and exchange contributions. The total interaction energies are also obtained by supermolecule coupled-cluster calculations including single, double, and noniterative triple excitations, CCSD(T). The three-body interactions contribute up to 28\% of the total interaction energy in these water clusters in their equilibrium geometries and up to 50\% of the barriers for different tunneling processes investigated in the trimer. The main three-body contribution is due to second- and third-order induction effects, but also three-body exchange effects are substantial. Dispersion contributions are only significant in the pair energy. The four-body effects are relatively small, and the five-body effects were found to be negligible. Furthermore, we tested the quality of various density functional methods for describing these many-body interactions.},
  doi =        {10.1021/jp990773d},
  file =       {Milet_et_al_1999_Hydrogen_Bonding_in_Water_Clusters3.pdf:/Users/tans/Dropbox/Papers/Office_zot_lib/The Journal of Physical Chemistry A/1999/Milet_et_al_1999_Hydrogen_Bonding_in_Water_Clusters3.pdf:application/pdf;Milet_et_al_1999_Hydrogen_Bonding_in_Water_Clusters4.pdf:/Users/tans/Dropbox/Papers/Office_zot_lib/The Journal of Physical Chemistry A/1999/Milet_et_al_1999_Hydrogen_Bonding_in_Water_Clusters4.pdf:application/pdf},
