@Article{Esrafili2012a,
  author =     {Esrafili, Mehdi D.},
  title =      {Characteristics and nature of the intermolecular interactions in boron-bonded complexes with carbene as electron donor: an ab initio, {SAPT} and {QTAIM} study},
  journal =    {Journal of Molecular Modeling},
  year =       {2012},
  volume =     {18},
  number =     {5},
  pages =      {2003--2011},
  month =      may,
  abstract =   {We report geometries, stabilization energies, symmetry adapted perturbation theory (SAPT) and quantum theory of atoms in molecules (QTAIM) analyses of a series of carbene–BX3 complexes, where X = H, OH, NH2, CH3, CN, NC, F, Cl, and Br. The stabilization energies were calculated at HF, B3LYP, MP2, MP4 and CCSD(T)/aug-cc-pVDZ levels of theory using optimized geometries of all the complexes obtained from B3LYP/aug-cc-pVTZ. Quantitatively, all the complexes indicate the presence of B–Ccarbene interaction due to the short B–Ccarbene distances. Inspection of stabilization energies reveals that the interaction energies increase in the order NH2 {\textbackslash}textgreater OH {\textbackslash}textgreater CH3 {\textbackslash}textgreater F {\textbackslash}textgreater H {\textbackslash}textgreater Cl {\textbackslash}textgreater Br {\textbackslash}textgreater NC {\textbackslash}textgreater CN, which is the opposite trend shown in the binding distances. Considering the SAPT results, it is found that electrostatic effects account for about 50\% of the overall attraction of the studied complexes. By comparison, the induction components of these interactions represent about 40\% of the total attractive forces. Despite falling in a region of charge depletion with ∇2 ρ BCP {\textbackslash}textgreater0, the B–Ccarbene bond critical points (BCPs) are characterized by a reasonably large value of the electron density (ρ BCP) and HBCP {\textbackslash}textless0, indicating that the potential energy overcomes the kinetic energy density at BCP and the B–Ccarbene bond is a polar covalent bond.},
  doi =        {10.1007/s00894-011-1221-2},
  file =       {Esrafili_2012_Characteristics_and_nature_of_the_intermolecular_interactions_in_boron-bonded2.pdf:/Users/tans/Dropbox/Papers/Office_zot_lib/Journal of Molecular Modeling/2012/Esrafili_2012_Characteristics_and_nature_of_the_intermolecular_interactions_in_boron-bonded2.pdf:application/pdf},
  issn =       {1610-2940, 0948-5023},
  keywords =   {Ab initio, Biomedicine general, Carbene, Computer Applications in Chemistry, Computer Appl. in Life Sciences, general, Health Informatics, Life Sciences, Molecular Medicine, QTAIM, Symmetry-adapted perturbation theory},
  language =   {en},
  shorttitle = {Characteristics and nature of the intermolecular interactions in boron-bonded complexes with carbene as electron donor},
  url =        {http://link.springer.com/article/10.1007/s00894-011-1221-2},
  urldate =    {2014-06-25}
}

