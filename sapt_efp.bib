  file =     {Yu_et_al_2013_Conductivity,_Spectroscopic,_and_Computational_Investigation_of_H3O+_Solvation2.pdf:/Users/tans/Dropbox/Papers/Office_zot_lib/The Journal of Physical Chemistry B/2013/Yu_et_al_2013_Conductivity,_Spectroscopic,_and_Computational_Investigation_of_H3O+_Solvation2.pdf:application/pdf},
